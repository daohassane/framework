{
    "name": "mars/mars",
    "description": "Mars Framework",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/TheMartianGeeks/mars/issues",
        "source": "https://github.com/TheMartianGeeks/mars"
    },
    "authors": [
        {
            "name": "Houssene Dao",
            "email": "dao.houssene@gmail.com"
        },
        {
            "name": "Hassane Dao",
            "email": "dao.hassane@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Mars\\": "src/Mars/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mars\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
